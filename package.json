{
  "name": "turborepo-basic-shared",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "clean": "turbo run clean",
    "start": "turbo run start",
    "build": "turbo run build",
    "start:bot": "turbo run start:bot",
    "start:web": "turbo run start:web",
    "dev:web": "turbo run dev:web",
    "dev:bot": "turbo run dev:bot",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate": "turbo run generate",
    "db:push": "turbo run db:push",
    "daemon": "turbo run daemon",
    "studio": "turbo run studio",
    "composedb:daemon": "turbo run composedb:daemon",
    "composedb:server": "turbo run composedb:server",
    "composedb:server:docker": "turbo run composedb:server:docker"
  },
  "devDependencies": {
    "prettier": "^2.8.1",
    "turbo": "^1.6.3"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
